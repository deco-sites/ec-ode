{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.8rem",
    "--btn-focus-scale": "0.95",
    "--animation-btn": "0.25s",
    "--btn-text-case": "normal-case"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "font": {
    "fonts": [
      {
        "family": "Poppins",
        "variations": [
          {
            "italic": false,
            "weight": "400"
