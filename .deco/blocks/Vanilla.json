  },
  "fonts": [
    {
      "fontFamily": "Poppins"
    }
  ],
  "mainColors": {
    "neutral": "#1d5903",
    "primary": "#2c812e",
    "warning": "#e08e00",
    "base-100": "#fefae1",
    "tertiary": "#2C812E",
    "secondary": "#fefadf"
